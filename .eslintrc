{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "jest": true
  },
  "rules": {
    "no-unused-vars": 0,
    "no-underscore-dangle": 0,
    "max-len": 0,
    "no-shadow": 0,
    "import/no-extraneous-dependencies": 0,
    "no-plusplus": 0,
    "import/no-dynamic-require": 0,
    "no-use-before-define": 0,
    "func-names": 0,
    "no-param-reassign": 0,
    "global-require": 0,
    "eqeqeq": 0,
    "camelcase": 0,
    "no-mixed-operators": 0,
    "no-console": 0,
    "no-bitwise": 0,
    "prefer-promise-reject-errors": 0,
    "no-await-in-loop": 0,
    "consistent-return": 0,
    "radix": 0,
    "no-nested-ternary": 0
  }
}
